        "label": "User Role",
        "datasource": {"type": "postgres", "uid": "PostgreSQL-Wildfire"},
        "query": "SELECT user_role as __value, CASE WHEN user_role = 'admin' THEN 'Admin' WHEN user_role = 'fire_chief' THEN 'Fire Chief' WHEN user_role = 'data_scientist' THEN 'Data Scientist' WHEN user_role = 'data_analyst' THEN 'Data Analyst' WHEN user_role = 'field_responder' THEN 'Field Responder' END as __text FROM (SELECT 'admin' as user_role UNION SELECT 'fire_chief' UNION SELECT 'data_scientist' UNION SELECT 'data_analyst' UNION SELECT 'field_responder') t ORDER BY CASE user_role WHEN 'admin' THEN 1 WHEN 'fire_chief' THEN 2 WHEN 'data_scientist' THEN 3 WHEN 'data_analyst' THEN 4 WHEN 'field_responder' THEN 5 END",
        "refresh": 1,
        "regex": "",
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "skipUrlSync": false,
        "sort": 0
      }
    ]
